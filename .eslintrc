{
  parser: "babel-eslint",
 "plugins": [
    "react",
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/no-string-refs": [0],
    "comma-dangle": [1, "always-multiline"],
    "max-len": [0, 180, 2],
    "prop-types": [2],
    "arrow-body-style": [0],
    "import/no-unresolved": [0],
    "no-underscore-dangle": 0,
    "import/no-named-as-default": 0,
    "react/jsx-tag-spacing": [
      "error",
      {
        "beforeSelfClosing": "always",
      }
    ],
  }
}
